<?xml version="1.0" encoding="UTF-8"?>
<SystemDefinition xmlns:sf="http://www.symbian.org/system-definition" schema="3.0.0" id-namespace="http://www.nokia.com/schema/drd/system-definition">
  <package id="sf:usbservices" name="USB Services" levels="framework server generic specific plugin">
    <collection id="sf:usbclasses" name="USB classes" level="framework">
      <component id="usbosdescriptorhandler" filter="s60" name="USB OS Descriptor Handler" class="plugin">
        <unit bldFile="../../usbclasses/usbosdescriptorhandler/group"/>
      </component>
      <component id="msmmplugin" filter="s60" name="Mass Storage Mount Manager Plugin" class="plugin" introduced="^3">
        <unit bldFile="../../usbclasses/msmmplugin/group"/>
      </component>
      <component id="usbhidclassdriver" filter="s60" name="USB HID Class Driver" class="plugin" introduced="^3">
        <unit bldFile="../../usbclasses/usbhidclassdriver/group"/>
      </component>
    </collection>
    <collection id="sf:usbuis" name="USB UIs" level="specific">
      <component id="rndisui" filter="s60" name="Remote NDIS UI" class="plugin" introduced="^3">
        <unit bldFile="../../usbuis/rndisui/group"/>
      </component>
    </collection>
    <collection id="sf:usbservices_info" name="USB Services Info" level="plugin">
     <component id="nokia_usbservices_metadata" filter="s60" name="Nokia USB Services Metadata" class="config" introduced="^2" purpose="development" target="desktop">
        <unit base="../nokia_usbservices_metadata"/>
      </component>
    </collection>
  </package>
</SystemDefinition>
